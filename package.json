{
  "name": "pitang-internship-challenge-backend",
  "version": "1.0.0",
  "description": "\"COVID 19 Vaccination Scheduler API\"",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts | pino-pretty",
    "build": "tsup src",
    "start": "node dist/server.js | pino-pretty",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test:unit": "vitest run --dir tests/repositories && vitest run --dir tests/validators",
    "test:unit:watch": "vitest --dir tests/validators",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegopluna/pitang-internship-challenge-backend.git"
  },
  "author": "Diego Luna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diegopluna/pitang-internship-challenge-backend/issues"
  },
  "homepage": "https://github.com/diegopluna/pitang-internship-challenge-backend#readme",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.16.1",
    "supertest": "^7.0.0",
    "tsup": "^8.1.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@prisma/client": "^5.16.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "pino-pretty": "^11.2.1",
    "zod": "^3.23.8"
  }
}
